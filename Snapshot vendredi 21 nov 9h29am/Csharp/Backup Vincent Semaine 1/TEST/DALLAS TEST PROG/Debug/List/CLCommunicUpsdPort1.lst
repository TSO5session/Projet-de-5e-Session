    307          //*****************************************************************************
    308          void CLCommunic :: SendString(char const *ucpMessage)
    309          {
    310          while(*ucpMessage != 0x00)          // Tant qu'on a pas atteint la fin de 
    311            {                                 // la chaine.
    312             Send(*ucpMessage);     // Transmet le caractere.
    313             ucpMessage++;                    // Passe au caractere suivant.
    314            }
    315          }
