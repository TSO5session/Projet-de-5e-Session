        CFI CFA_SP SP+0
        CFI CFA_XSP16 add(XSP16, 8)
        ; Saved register size: 8
        ; Auto size: 0
        MOV     A,R2
        MOV     R6,A
        MOV     A,R3
        MOV     R7,A
        MOV     DPL,R4
        MOV     DPH,R5
        SJMP    ??vAffChaine_1
//  309 while(*ucpMessage != 0x00)          // Tant qu'on a pas atteint la fin de 
//  310   {                                 // la chaine.
//  311    vTransCaratere(*ucpMessage);     // Transmet le caractere.
??vAffChaine_2:
        ; Setup parameters for call to function CLCommunic::vTransCaratere(UC)
        MOVX    A,@DPTR
        MOV     R1,A
        MOV     A,R6
        MOV     R2,A
        MOV     A,R7
        MOV     R3,A
        LCALL   ??vTransCaratere
//  312    ucpMessage++;                    // Passe au caractere suivant.
        INC     DPTR
//  313   }
??vAffChaine_1:
        MOVX    A,@DPTR
        JNZ     ??vAffChaine_2
//  314 }
        MOV     R7,#0x1
        LJMP    ?FUNC_LEAVE_XDATA
        CFI EndBlock cfiBlock9
