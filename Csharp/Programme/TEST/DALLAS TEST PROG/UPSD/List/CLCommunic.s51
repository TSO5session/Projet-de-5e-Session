//  536    ucCaractereRecu = SBUF0;
        MOV     A,0x99
        MOV     DPTR,#??ucCaractereRecu
        MOVX    @DPTR,A
//  537    ucEtatReception = ACTIF;
        MOV     DPTR,#??ucEtatReception
        MOV     A,#0x1
        MOVX    @DPTR,A
//  538    cptrEcriture++;           // Increment la position du pointeur d'ecriture.
        MOV     DPTR,#??cptrEcriture
        MOVX    A,@DPTR
        ADD     A,#0x1
        MOV     R0,A
        INC     DPTR
        MOVX    A,@DPTR
        ADDC    A,#0x0
        MOV     R1,A
        MOV     DPTR,#??cptrEcriture
        MOV     A,R0
        MOVX    @DPTR,A
        INC     DPTR
        MOV     A,R1
        MOVX    @DPTR,A
//  539     
//  540    if(ucLireNbCaractRecu() < (ucLongueurTrame - 1) )
        ; Setup parameters for call to function CLCommunic::ucLireNbCaractRecu()
        LCALL   ??ucLireNbCaractRecu
        MOV     A,R1
        MOV     R2,A
        MOV     R3,#0x0
        MOV     DPTR,#??ucLongueurTrame
        MOVX    A,@DPTR
        MOV     R0,A
        MOV     R1,#0x0
        ADD     A,#-0x1
        MOV     R0,A
        MOV     A,R1
        ADDC    A,#-0x1
        MOV     R1,A
        CLR     C
        MOV     A,R2
        SUBB    A,R0
        MOV     A,R3
        SUBB    A,R1
        MOV     C,0xD0 /* PSW */.2
        XRL     A,PSW
        RLC     A
        JNC     ??vInteruptSerial_3
//  541      {                       // Si le nombre de caractere recu est inferieur a 
//  542       ucNbCaractRecu++;      // trame - 1 incremente le nombre de caracteres 
        MOV     DPTR,#??ucNbCaractRecu
        MOVX    A,@DPTR
        ADD     A,#0x1
        MOVX    @DPTR,A
        SJMP    ??vInteruptSerial_4
//  543      }                       // recus.
//  544    else                      // Si la trame est complete?
//  545      {
//  546       ucNbCaractRecu=0;      // Remet a 0 le compte de caractere.
??vInteruptSerial_3:
        MOV     DPTR,#??ucNbCaractRecu
        MOV     A,#0x0
        MOVX    @DPTR,A
//  547       ucEtatTrame = COMPLET; // Indique qu'une trame est complete.
        MOV     DPTR,#??ucEtatTrame
        MOV     A,#0x1
        MOVX    @DPTR,A
//  548       cptrLecture = (cptrEcriture - (ucLongueurTrame)); 
        MOV     DPTR,#??ucLongueurTrame
        MOVX    A,@DPTR
        MOV     R0,A
        MOV     R1,#0x0
        CLR     C
        CLR     A
        SUBB    A,R0
        MOV     R0,A
        CLR     A
        SUBB    A,R1
        MOV     R1,A
        MOV     DPTR,#??cptrEcriture
        MOVX    A,@DPTR
        ADD     A,R0
        MOV     R0,A
        INC     DPTR
        MOVX    A,@DPTR
        ADDC    A,R1
        MOV     R1,A
        MOV     DPTR,#??cptrLecture
        MOV     A,R0
        MOVX    @DPTR,A
        INC     DPTR
        MOV     A,R1
        MOVX    @DPTR,A
//  549                              // Le pointeur de lecture = la position 
//  550                              // d'ecriture - la longuer de la trame.
//  551      }
//  552    RI_0 = 0;                 // Reinitialiser le flag de reception.
??vInteruptSerial_4:
        CLR     0x98.0
//  553  }                           // Fin du if(RI_0 = 1)
//  554 EA = 1;                      // Reactive les interruptions
??vInteruptSerial_1:
        SETB    0xa8.7
//  555 }                            // Fin de Interruption.
        MOV     R7,#0x1
        LJMP    ?INTERRUPT_LEAVE_XSP
        CFI EndBlock cfiBlock14
