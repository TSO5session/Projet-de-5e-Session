        ; Saved register size: 15
        ; Auto size: 0
//  526 EA = 0;                               // Desactive les interruptions.
        CLR     0xa8.7
//  527 
//  528 if(RI_0==1)                           // Interruption cause par la reception?
        MOV     C,0x98.0
        JC      $+5
        LJMP    ??vInteruptSerial_1
//  529   {
//  530    if(cptrEcriture > cpFinTrame)      // Si on a atteind la fin du buffer
        MOV     DPTR,#??cpFinTrame
        PUSH    DPL
        CFI CFA_SP SP+-4
        PUSH    DPH
        CFI CFA_SP SP+-5
        MOV     DPTR,#??cptrEcriture
        MOVX    A,@DPTR
        MOV     R0,A
        INC     DPTR
        MOVX    A,@DPTR
        MOV     R1,A
        POP     DPH
        CFI CFA_SP SP+-4
        POP     DPL
        CFI CFA_SP SP+-3
        CLR     C
        MOVX    A,@DPTR
        SUBB    A,R0
        INC     DPTR
        MOVX    A,@DPTR
        SUBB    A,R1
        JNC     ??vInteruptSerial_2
//  531      {
//  532       cptrEcriture = cpDebutTrame;    // Retourne au debut du tampon.
        MOV     DPTR,#??cpDebutTrame
        MOVX    A,@DPTR
        MOV     R0,A
        INC     DPTR
        MOVX    A,@DPTR
        MOV     R1,A
        MOV     DPTR,#??cptrEcriture
        MOV     A,R0
        MOVX    @DPTR,A
        INC     DPTR
        MOV     A,R1
        MOVX    @DPTR,A
//  533      }      
//  534      
//  535   *cptrEcriture    = SBUF0;  // Lit dans SBUF0 le caractere recu et met dans le tableau.                               
??vInteruptSerial_2:
        MOV     A,0x99
        PUSH    A
        CFI CFA_SP SP+-4
        MOV     DPTR,#??cptrEcriture
        MOVX    A,@DPTR
        MOV     R0,A
        INC     DPTR
        MOVX    A,@DPTR
        MOV     DPH,A
        MOV     DPL,R0
        POP     A
        CFI CFA_SP SP+-3
