//  501 //*****************************************************************************
//  502 // Nom de la fct:   vInteruptSerial                  Fait par: VIncent CHouinard
//  503 // Date:            2 octobre 2013
//  504 //
//  505 // Revision :       B
//  506 // Modification :   10 avril 2014     conversion pour IAR
//  507 //
//  508 // INCLUDE:         #include "_TypeCible.h", "CLCommunic.h"
//  509 // Prototype:       __interrupt  void CLCommunic:: vInteruptSerial(void);
//  510 //
//  511 // Description:     Fonction qui permet de recevoir des caracteres par le port
//  512 //                  serie, si l'interruption vient de la reception on place
//  513 //                  les caracteres lus dans un tableau.
//  514 //
//  515 // Parametre d'entree: aucun.
//  516 //
//  517 //
//  518 // Parametre de sortie: valeur dans cptrLecture pour lire les caractere recus.
//  519 //
//  520 // Procedure appelees: ucLireNbCaractRecu(),
//  521 //
//  522 // Exemple d'appel: aucun
//  523 //*****************************************************************************
//  524 #pragma vector=0x23  // Interrupt 4 pour interruption serie au vecteur 23

        RSEG NEAR_CODE:CODE:NOROOT(0)
//  525  __interrupt  void CLCommunic:: vInteruptSerial(void)
??vInteruptSerial:
        CFI Block cfiBlock14 Using cfiCommon1
        CFI Function ??vInteruptSerial
        CODE
//  526 {
        FUNCALL ??vInteruptSerial, ??ucLireNbCaractRecu
        LOCFRAME ISTACK, 15, STACK
        LOCFRAME PSTACK, 0, STACK
        LOCFRAME XSTACK, 0, STACK
        LOCFRAME IOVERLAY, 0, STATIC
        LOCFRAME DOVERLAY, 0, STATIC
        ARGFRAME ISTACK, 15, STACK
        ARGFRAME PSTACK, 0, STACK
        ARGFRAME XSTACK, 0, STACK
        ARGFRAME IOVERLAY, 0, STATIC
        ARGFRAME DOVERLAY, 0, STATIC
        PUSH    A
        CFI A Frame(CFA_SP, 3)
        CFI CFA_SP SP+-3
        MOV     A,#-0xe
        LCALL   ?INTERRUPT_ENTER_XSP
        CFI B load(1, XDATA, add(CFA_XSP16, literal(-1)))
        CFI VB load(1, XDATA, add(CFA_XSP16, literal(-2)))
        CFI PSW load(1, XDATA, add(CFA_XSP16, literal(-3)))
        CFI DPL0 load(1, XDATA, add(CFA_XSP16, literal(-4)))
        CFI DPH0 load(1, XDATA, add(CFA_XSP16, literal(-5)))
        CFI R7 load(1, XDATA, add(CFA_XSP16, literal(-6)))
        CFI R6 load(1, XDATA, add(CFA_XSP16, literal(-7)))
        CFI R5 load(1, XDATA, add(CFA_XSP16, literal(-8)))
        CFI R4 load(1, XDATA, add(CFA_XSP16, literal(-9)))
        CFI V0 load(1, XDATA, add(CFA_XSP16, literal(-10)))
        CFI R3 load(1, XDATA, add(CFA_XSP16, literal(-11)))
        CFI R2 load(1, XDATA, add(CFA_XSP16, literal(-12)))
        CFI R1 load(1, XDATA, add(CFA_XSP16, literal(-13)))
        CFI R0 load(1, XDATA, add(CFA_XSP16, literal(-14)))
        CFI CFA_XSP16 add(XSP16, 14)
        ; Saved register size: 15
        ; Auto size: 0
//  527 EA = 0;                               // Desactive les interruptions.
        CLR     0xa8.7
//  528 
//  529 if(RI_0==1)                           // Interruption cause par la reception?
        MOV     C,0x98.0
        JC      $+5
        LJMP    ??vInteruptSerial_1
//  530   {
//  531    if(cptrEcriture > cpFinTrame)      // Si on a atteind la fin du buffer
        MOV     DPTR,#??cpFinTrame
        PUSH    DPL
        CFI CFA_SP SP+-4
        PUSH    DPH
        CFI CFA_SP SP+-5
        MOV     DPTR,#??cptrEcriture
        MOVX    A,@DPTR
        MOV     R0,A
        INC     DPTR
        MOVX    A,@DPTR
        MOV     R1,A
        POP     DPH
        CFI CFA_SP SP+-4
        POP     DPL
        CFI CFA_SP SP+-3
        CLR     C
        MOVX    A,@DPTR
        SUBB    A,R0
        INC     DPTR
        MOVX    A,@DPTR
        SUBB    A,R1
        JNC     ??vInteruptSerial_2
//  532      {
//  533       cptrEcriture = cpDebutTrame;    // Retourne au debut du tampon.
        MOV     DPTR,#??cpDebutTrame
        MOVX    A,@DPTR
        MOV     R0,A
        INC     DPTR
        MOVX    A,@DPTR
        MOV     R1,A
        MOV     DPTR,#??cptrEcriture
        MOV     A,R0
        MOVX    @DPTR,A
        INC     DPTR
        MOV     A,R1
        MOVX    @DPTR,A
//  534      }
//  535 
//  536   *cptrEcriture    = SBUF0;  // Lit dans SBUF0 le caractere recu et met dans le tableau.
??vInteruptSerial_2:
        MOV     A,0x99
        PUSH    A
        CFI CFA_SP SP+-4
        MOV     DPTR,#??cptrEcriture
        MOVX    A,@DPTR
        MOV     R0,A
        INC     DPTR
        MOVX    A,@DPTR
        MOV     DPH,A
        MOV     DPL,R0
        POP     A
        CFI CFA_SP SP+-3
        MOVX    @DPTR,A
//  537    ucCaractereRecu = SBUF0;
        MOV     A,0x99
        MOV     DPTR,#??ucCaractereRecu
        MOVX    @DPTR,A
//  538    ucEtatReception = ACTIF;
        MOV     DPTR,#??ucEtatReception
        MOV     A,#0x1
        MOVX    @DPTR,A
//  539    cptrEcriture++;           // Increment la position du pointeur d'ecriture.
        MOV     DPTR,#??cptrEcriture
        MOVX    A,@DPTR
        ADD     A,#0x1
        MOV     R0,A
        INC     DPTR
        MOVX    A,@DPTR
        ADDC    A,#0x0
        MOV     R1,A
        MOV     DPTR,#??cptrEcriture
        MOV     A,R0
        MOVX    @DPTR,A
        INC     DPTR
        MOV     A,R1
        MOVX    @DPTR,A
//  540 
//  541    if(ucLireNbCaractRecu() < (ucLongueurTrame - 1) )
        ; Setup parameters for call to function CLCommunic::ucLireNbCaractRecu()
        LCALL   ??ucLireNbCaractRecu
        MOV     A,R1
        MOV     R2,A
        MOV     R3,#0x0
        MOV     DPTR,#??ucLongueurTrame
        MOVX    A,@DPTR
        MOV     R0,A
        MOV     R1,#0x0
        ADD     A,#-0x1
        MOV     R0,A
        MOV     A,R1
        ADDC    A,#-0x1
        MOV     R1,A
        CLR     C
        MOV     A,R2
        SUBB    A,R0
        MOV     A,R3
        SUBB    A,R1
        MOV     C,0xD0 /* PSW */.2
        XRL     A,PSW
        RLC     A
        JNC     ??vInteruptSerial_3
//  542      {                       // Si le nombre de caractere recu est inferieur a
//  543       ucNbCaractRecu++;      // trame - 1 incremente le nombre de caracteres
        MOV     DPTR,#??ucNbCaractRecu
        MOVX    A,@DPTR
        ADD     A,#0x1
        MOVX    @DPTR,A
        SJMP    ??vInteruptSerial_4
//  544      }                       // recus.
//  545    else                      // Si la trame est complete?
//  546      {
//  547       ucNbCaractRecu=0;      // Remet a 0 le compte de caractere.
??vInteruptSerial_3:
        MOV     DPTR,#??ucNbCaractRecu
        MOV     A,#0x0
        MOVX    @DPTR,A
//  548       ucEtatTrame = COMPLET; // Indique qu'une trame est complete.
        MOV     DPTR,#??ucEtatTrame
        MOV     A,#0x1
        MOVX    @DPTR,A
//  549       cptrLecture = (cptrEcriture - (ucLongueurTrame));
        MOV     DPTR,#??ucLongueurTrame
        MOVX    A,@DPTR
        MOV     R0,A
        MOV     R1,#0x0
        CLR     C
        CLR     A
        SUBB    A,R0
        MOV     R0,A
        CLR     A
        SUBB    A,R1
        MOV     R1,A
        MOV     DPTR,#??cptrEcriture
        MOVX    A,@DPTR
        ADD     A,R0
        MOV     R0,A
        INC     DPTR
        MOVX    A,@DPTR
        ADDC    A,R1
        MOV     R1,A
        MOV     DPTR,#??cptrLecture
        MOV     A,R0
        MOVX    @DPTR,A
        INC     DPTR
        MOV     A,R1
        MOVX    @DPTR,A
//  550                              // Le pointeur de lecture = la position
//  551                              // d'ecriture - la longuer de la trame.
//  552      }
//  553    RI_0 = 0;                 // Reinitialiser le flag de reception.
??vInteruptSerial_4:
        CLR     0x98.0
//  554  }                           // Fin du if(RI_0 = 1)
//  555 EA = 1;                      // Reactive les interruptions
??vInteruptSerial_1:
        SETB    0xa8.7
//  556 }                            // Fin de Interruption.
        MOV     R7,#0x1
        LJMP    ?INTERRUPT_LEAVE_XSP
        CFI EndBlock cfiBlock14
        REQUIRE _A_IE
        REQUIRE _A_SCON
        REQUIRE SBUF
