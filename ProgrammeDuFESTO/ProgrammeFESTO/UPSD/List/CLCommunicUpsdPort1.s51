??SendString_2:
        ; Setup parameters for call to function CLCommunic::Send(UC)
        MOVX    A,@DPTR
        MOV     R1,A
        MOV     A,R6
        MOV     R2,A
        MOV     A,R7
        MOV     R3,A
        LCALL   ??Send
//  313    ucpMessage++;                    // Passe au caractere suivant.
        INC     DPTR
//  314   }
??SendString_1:
        MOVX    A,@DPTR
        JNZ     ??SendString_2
