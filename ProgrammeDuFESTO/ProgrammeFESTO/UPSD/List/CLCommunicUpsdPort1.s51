//  443    break;
//  444    case 38400:
//  445       TH1= 0xFE;
//  446    break;
//  447    case 19200:
//  448       TH1= 0xFD;
//  449    break;
//  450    case 9600:
//  451       TH1= 0xFA;
//  452    break;
//  453    case 4800:
//  454       TH1= 0xF4;
//  455    break;
//  456    case 2400:
//  457       TH1= 0xE8;
//  458    break;
//  459    default:
//  460       TH1= 0xFA;        // Si aucune valeur ne correspond vitesse = 9600 .
//  461    break;
//  462   }
//  463 #endif
//  464 
//  465 #ifdef UPSD3254A        // Si la cible est le uPSD
//  466 
//  467 switch(uiBaud)         // switch case pour determiner la valeur en
        MOV     ?V0 + 0,R4
        MOV     ?V0 + 1,R5
        MOV     R0,#?V0 + 0
        LCALL   ?US_SWITCH_SPARSE
