        RSEG NEAR_CODE:CODE:NOROOT(0)
//  185 UC CLCommunic :: ucInitLongTrame(UC ucLong)
??ucInitLongTrame:
        CFI Block cfiBlock6 Using cfiCommon0
        CFI Function ??ucInitLongTrame
        CODE
//  186 {
        PUSH    DPL
        CFI DPL0 Frame(CFA_SP, 3)
        CFI CFA_SP SP+-3
        PUSH    DPH
        CFI DPH0 Frame(CFA_SP, 4)
        CFI CFA_SP SP+-4
        ; Saved register size: 2
        ; Auto size: 0
        MOV     A,R1
        MOV     R0,A
//  187 UC ucValide;                           // Varaible pour valider ou non.
//  188 
//  189 if(ucLong <= ucLongueurTampon)         // Si longueur trame voulue <= a la longueur
        MOV     DPTR,#??ucLongueurTampon
        MOVX    A,@DPTR
        CLR     C
        SUBB    A,R0
        JC      ??ucInitLongTrame_1
//  190   {                                    // du tampon.
//  191    ucLongueurTrame = ucLong;           // longueur trame = longueur trame voulue.
        MOV     A,R0
        MOV     DPTR,#??ucLongueurTrame
        MOVX    @DPTR,A
//  192    ucValide = VRAI;                    // ucValide = Vrai.
        MOV     R1,#0x1
//  193   }
//  194 
//  195 if(ucLong > ucLongueurTampon)          // Si longueur trame voulue > a la longueur
??ucInitLongTrame_1:
        MOV     DPTR,#??ucLongueurTampon
        MOVX    A,@DPTR
        CLR     C
        SUBB    A,R0
        JNC     ??ucInitLongTrame_2
//  196   {                                    // du tampon.
//  197    ucLongueurTrame = ucLongueurTampon; // longueur trame = longueur tampon.
        MOV     DPTR,#??ucLongueurTampon
        MOVX    A,@DPTR
        MOV     DPTR,#??ucLongueurTrame
        MOVX    @DPTR,A
//  198    ucValide = FAUX;                    // ucValide = Faux.
        MOV     R1,#0x0
//  199   }
//  200 
//  201 return(ucValide);                      // Retourne etat de ucValide.
??ucInitLongTrame_2:
        POP     DPH
        CFI DPH0 SameValue
        CFI CFA_SP SP+-3
        POP     DPL
        CFI DPL0 SameValue
        CFI CFA_SP SP+-2
        RET
        CFI EndBlock cfiBlock6
//  202 }
